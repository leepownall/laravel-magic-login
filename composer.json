{
    "name": "pownall/laravel-magic-login",
    "description": "Allow users to login via a magic login.",
    "keywords": [
        "magic-login"
    ],
    "homepage": "https://github.com/leepownall/magic-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Pownall",
            "email": "pownall@hey.com",
            "homepage": "https://leepownall.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0|^8.0",
        "spatie/url": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Pownall\\MagicLogin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pownall\\MagicLogin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pownall\\MagicLogin\\MagicLoginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
