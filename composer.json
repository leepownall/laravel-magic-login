{
    "name": "pownall/laravel-magic-login",
    "description": "Passwordless login for Laravel. Allows users to login via a magic link.",
    "keywords": [
        "passwordless",
        "passwordless-laravel",
        "magic-login",
        "magic-login-laravel"
    ],
    "homepage": "https://github.com/leepownall/magic-login",
    "license": "MIT",
    "authors": [
        {
            "name": "Lee Pownall",
            "email": "pownall@hey.com",
            "homepage": "https://leepownall.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "spatie/url": "^1.3"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^5.2 || ^6.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Pownall\\MagicLogin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pownall\\MagicLogin\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pownall\\MagicLogin\\MagicLoginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
